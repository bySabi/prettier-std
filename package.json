{
  "name": "prettier-std",
  "version": "1.0.1",
  "description": "prettier -> standard --fix -> Formatted Code",
  "keywords": [
    "prettier",
    "eslint",
    "prettier-eslint",
    "linter",
    "standard",
    "standard-engine",
    "semistandard"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "lint": "standard --fix --verbose | snazzy",
    "test": "npm run lint",
    "format": "prettier-std --no-bracket-spacing --write '**/*.js'"
  },
  "dependencies": {
    "prettier-eslint": "*",
    "standard-prettier-eslint": "1.x.x"
  },
  "devDependencies": {
    "standard": "*",
    "snazzy": "*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bySabi/prettier-std.git"
  },
  "bugs": {
    "url": "https://github.com/bySabi/prettier-std/issues"
  },
  "homepage": "https://github.com/bySabi/prettier-std#readme",
  "author": "bySabi Files",
  "license": "MIT"
}
